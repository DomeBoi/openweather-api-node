{
  "name": "openweather-api-node",
  "version": "1.4.2",
  "description": "Simple Node.js package that makes it easy to work with OpenWeather API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "docs": "typedoc ./src/index.ts --out ./docs --media ./media --customCss ./media/style.css",
    "docs:deploy": "gh-pages -d ./docs"
  },
  "keywords": [
    "weather",
    "openweather",
    "openweathermap",
    "meteo",
    "weather-api",
    "air",
    "pollution",
    "api"
  ],
  "author": "loloToster",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "gh-pages": "^4.0.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.22",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loloToster/openweather-api-node.git"
  },
  "bugs": {
    "url": "https://github.com/loloToster/openweather-api-node/issues"
  },
  "homepage": "https://github.com/loloToster/openweather-api-node#readme",
  "directories": {
    "example": "examples",
    "test": "test"
  }
}
